{
  "expo": {
    "name": "CodOcean-ReactNative",
    "slug": "CodOcean-ReactNative",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-white.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo-text.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "assetBundlePatterns": ["**/*"],
    "fonts": {
      "SairaSemiCondensed-Thin": "./assets/fonts/SairaSemiCondensed-Thin.ttf",
      "SairaSemiCondensed-ExtraLight": "./assets/fonts/SairaSemiCondensed-ExtraLight.ttf"
    }
  }
}
